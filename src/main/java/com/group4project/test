		<![CDATA[SELECT
    o.order_no,
    o.order_date,
    o.total_amount,
    o.fk_staff_id,
    oi.fk_menu_id,
    mi.menu_name,
    oi.quantity,
    oi.price,
    oi.total_price
FROM orders o
JOIN orderDetails oi ON o.order_no = oi.fk_order_no
JOIN menu mi ON oi.fk_menu_id = mi.menu_id
WHERE o.order_no =   $P{orderNo}]]>
	</queryString>
