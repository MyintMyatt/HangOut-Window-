<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane minWidth="193.0" prefHeight="244.0" prefWidth="193.0" styleClass="foodPhoto-pane" stylesheets="@css/home.css" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.group4project.FoodPaneController">
    <children>
        <ImageView fx:id="image" fitHeight="133.0" fitWidth="128.0" layoutX="35.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="foodName" layoutX="14.0" layoutY="145.0" prefHeight="42.0" prefWidth="87.0" wrapText="true">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label fx:id="price" layoutX="99.0" layoutY="145.0" prefHeight="42.0" prefWidth="87.0" styleClass="price-label">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Spinner fx:id="spinner" layoutX="14.0" layoutY="193.0" onMouseClicked="#spinnerAction" prefHeight="35.0" prefWidth="87.0" />
        <Button layoutX="115.0" layoutY="193.0" mnemonicParsing="false" onAction="#addBtnAction" prefHeight="35.0" prefWidth="56.0" styleClass="food-add-btn" text="ADD" />
    </children>
</Pane>
